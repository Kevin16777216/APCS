import java.util.Scanner;
public class ReverseDigits{
    public static void main(){
	Scanner sc = new Scanner(System.in);
	System.out.print("Give me a number:");
	int input = sc.nextInt();
	int result = 0;
	while (input != 0){
	    result += input % 10;
	    result *= 10;
	    input /= 10;
	}
	System.out.println("Result:" + result);
    }
}
