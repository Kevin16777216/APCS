public class SkiJumper extends Athlete implements Flier{
    private int jumps = 0;
    public SkiJumper(String first, String last){
	super(first,last);
    }
    public void fly(){
	jumps++;
    }
    public int getJumps(){
	return jumps;
    }
    @Override
    public String toString(){
	return super.toString() + " jumps : " + jumps;
    }
}
