public class Lab13{
    public static void main(String[] args){
	System.out.println(reverse("racecar"));
	System.out.println(reverse(" car"));
	System.out.println(reverse("     \n \n    "));
	
    }
    public static String reverse(String str){
	String res = "";
	for(int i = str.length()-1; i >= 0;i--){
	    res += str.charAt(i);
	}
	return res;
    }
}
